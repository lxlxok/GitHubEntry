<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="gitluck.com.githubentry.Activity.SettingActivity">
    <gitluck.com.githubentry.Bar.TopBar
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_gravity="top"
        custom:mytitle="Setting"
        custom:mytitleTextColor="@color/black"
        custom:mytitleTextSize="5dp"
        custom:leftText="返回"
        custom:leftTextColor="@color/black"
        custom:leftTextSize="5dp"
        custom:rightText="更多"
        custom:rightTextColor="@color/black"
        custom:rightTextSize="5dp"></gitluck.com.githubentry.Bar.TopBar>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="Setting"
        android:textSize="36sp"
        android:textStyle="bold"/>
</LinearLayout>
